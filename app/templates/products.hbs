
  <div class="content-container">
    <div class="content">
        {{#each @model as |product|}}
      <div class="card">
    <div class="card-img">
        <img class="item-img" src="{{product.image}}" />
    </div>
    <div class="card-details">
        <h2>{{product.title}}</h2>
        <div class="card-price-rate">
            <p>$ {{product.price}}</p>
            <p>{{product.rating.rate}}</p>
        </div>
    </div>
    <a class="button-outlined" href='/'>
        View Product
    </a>
</div>
{{/each}}
    </div>
  </div>
